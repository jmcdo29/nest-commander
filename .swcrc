{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es2017",
    "keepClassNames": true,
    "baseUrl": ".",
    "paths": {
      "nest-commander": ["packages/nest-commander/src/index.ts"],
      "nest-commander-testing": ["packages/nest-commander-testing/src/index.ts"]
    }
  },
  "module": {
    "type": "commonjs",
    "strictMode": true
  }
}
