{
  "name": "nest-commander-monorepo",
  "version": "0.0.0",
  "description": "A module for making CLI applications with NestJS. Decorators for running commands and separating out config parsers included. This package works on top of commander.",
  "scripts": {
    "build": "nx affected:build --with-deps --parallel",
    "test": "nx affected:test",
    "lint": "eslint --ext .ts .",
    "format": "prettier \"{packages,integration}/**/{src,test}/*.ts\"",
    "format:check": "pnpm format -- --check",
    "format:write": "pnpm format -- --write",
    "test:int": "nx run-many --target=e2e --all",
    "postinstall": "husky install",
    "release": "changeset publish",
    "nx": "nx",
    "deploy": "nx deploy docs"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmcdo29/nest-commander.git"
  },
  "keywords": [
    "cli",
    "nestjs",
    "application",
    "command",
    "command-line",
    "nest",
    "decorator"
  ],
  "author": "Jay McDoniel <jmcdo29@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmcdo29/nest-commander/issues"
  },
  "homepage": "https://github.com/jmcdo29/nest-commander#readme",
  "devDependencies": {
    "@angular-devkit/core": "^12.0.5",
    "@angular-devkit/schematics": "^12.0.5",
    "@angular-devkit/schematics-cli": "^12.0.5",
    "@changesets/cli": "^2.16.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@docusaurus/core": "^2.0.0-alpha.70",
    "@docusaurus/preset-classic": "^2.0.0-alpha.70",
    "@golevelup/nestjs-discovery": "^2.3.1",
    "@mdx-js/react": "^1.6.21",
    "@nestjs/cli": "^8.0.2",
    "@nestjs/common": "^8.0.1",
    "@nestjs/core": "^8.0.1",
    "@nestjs/testing": "^8.0.1",
    "@nrwl/cli": "latest",
    "@nrwl/jest": "^12.4.0",
    "@nrwl/node": "^12.4.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "latest",
    "@nrwl/workspace": "latest",
    "@nx-plus/docusaurus": "^12.0.0",
    "@types/inquirer": "^7.3.2",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "clsx": "^1.1.1",
    "commander": "^8.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.6",
    "inquirer": "^8.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1"
  },
  "resolutions": {
    "terser": "^4.0.0"
  }
}
