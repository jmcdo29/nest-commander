{
  "name": "nest-commander-monorepo",
  "version": "0.0.0",
  "description": "A module for making CLI applications with NestJS. Decorators for running commands and separating out config parsers included. This package works on top of commander.",
  "scripts": {
    "build": "nx affected:build",
    "test": "jest -c jest.config.js test",
    "lint": "eslint --ext .ts .",
    "format": "prettier \"{packages,integration}/**/{src,test}/*.ts\"",
    "format:check": "pnpm format -- --check",
    "format:write": "pnpm format -- --write",
    "test:int": "nx affected:e2e",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "publish": "pnpm -r publish --dry-run",
    "postpublish": "pinst --enable",
    "release": "pnpm build && changeset publish",
    "nx": "nx"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmcdo29/nestjs-commander.git"
  },
  "keywords": [
    "cli",
    "nestjs",
    "application",
    "command",
    "command-line",
    "nest",
    "decorator"
  ],
  "author": "Jay McDoniel <jmcdo29@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmcdo29/nestjs-commander/issues"
  },
  "homepage": "https://github.com/jmcdo29/nestjs-commander#readme",
  "devDependencies": {
    "@angular-devkit/core": "^12.0.5",
    "@angular-devkit/schematics": "^12.0.5",
    "@angular-devkit/schematics-cli": "^12.0.5",
    "@changesets/cli": "^2.16.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@golevelup/nestjs-discovery": "^2.3.1",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.6.5",
    "@nestjs/testing": "^7.6.5",
    "@nrwl/cli": "latest",
    "@nrwl/jest": "^12.4.0",
    "@nrwl/node": "^12.4.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "latest",
    "@nrwl/workspace": "latest",
    "@types/inquirer": "^7.3.2",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "commander": "^7.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.6",
    "inquirer": "^8.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  }
}
