    "@angular-devkit/schematics": "11.2.4",
    "@nestjs/schematics": "^7.3.1",
    "rxjs": "^6.6.7",
    "typescript": "~4.1.2"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.1102.8",
    "@types/jest": "^26.0.22",
