{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "outDir": "./dist",
    "baseUrl": ".",
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "lib": ["ES7"],
    "paths": {
      "nest-commander": ["packages/nest-commander/src/index.ts"],
      "nest-commander-testing": ["packages/nest-commander-testing/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
